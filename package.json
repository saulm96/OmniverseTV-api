{
  "name": "omniverse-tv-api",
  "version": "1.0.0",
  "description": "Backend service for an Interdimensional TV subscription platform.",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "nodemon",
    "build": "tsc",
    "scheduler": "ts-node src/scheduler.ts",
    "docker:dev": "scripts\\dev.bat",
    "docker:prod": "scripts\\prod.bat",
    "docker:clean": "scripts\\clean.bat",
    "docker:logs": "docker-compose logs -f",
    "docker:logs:scheduler": "docker-compose logs -f scheduler",
    "db:seed": "ts-node src/config/database/seed.ts",
    "docs": "typedoc",
    "docs:watch": "typedoc --watch",
    "docs:serve": "npx http-server docs -o"
  },
  "keywords": [
    "api",
    "backend",
    "nodejs",
    "typescript",
    "docker"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "bullmq": "^5.7.11",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.9.7",
    "nodemailer": "^7.0.6",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "redis": "^4.6.13",
    "sequelize": "^6.37.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.7",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.12.12",
    "@types/nodemailer": "^7.0.2",
    "@types/passport": "^1.0.17",
    "@types/passport-google-oauth20": "^2.0.16",
    "nodemon": "^3.1.0",
    "ts-node": "^10.9.2",
    "typedoc": "^0.28.13",
    "typescript": "^5.4.5"
  }
}
